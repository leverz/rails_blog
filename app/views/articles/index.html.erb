<h1>文章列表</h1>

<%= link_to 'New Article', new_article_path %>

<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Text</th>
		</tr>
	</thead>
	<tbody>
	<%= @articles.each do |article| %>
		<tr>
			<td><%= article.title %></td>
			<td><%= article.text %></td>
			<td><%= link_to 'Show', article_path(article) %></td>
		</tr>
	<% end %>
	</tbody>
</table>